<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="267.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.VisitorFormController">
   <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="25.0">
         <children>
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Name:" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Last Name:" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="ID:" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Phone:" />
            <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Email:" />
         </children>
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="207.0" spacing="15.0">
         <children>
            <TextField fx:id="nameText" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <TextField fx:id="lastNameText" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <TextField fx:id="idText" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <TextField fx:id="phoneText" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <TextField fx:id="emailText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
         </children>
         <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <VBox alignment="BOTTOM_CENTER" prefHeight="245.0" prefWidth="139.0">
         <children>
            <Button fx:id="backButton" alignment="CENTER" mnemonicParsing="false" onAction="#backBtn" text="Back">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="emailUpadteBtn" alignment="CENTER" mnemonicParsing="false" onAction="#updateEmailInDB" text="Update Email" />
            <Label fx:id="emailUpdatedText" text="email updated" textFill="#0d0d0d" visible="false" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="15.0" />
         </padding>
      </VBox>
   </children>
</HBox>
